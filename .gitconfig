[remote "QuenLo"]
    url = https://github.com/QuenLo/LeetCode-share.git
    fetch = +refs/heads/*:refs/remotes/QuenLo/*
[remote "origin"]
    url = git@github.com:huangyingw/LeetCode-share.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = QuenLo
    branch = master
[merge]
	allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[commit]
	message = n
[vrun]
	silent = false
	async = false
[deploy]
	host = localhost
	path = ~/myproject/git/interview/leetcode_solutions/QuenLo/LeetCode-share
